<%- include('partials/header') %>
<h1>Home</h1>
<p> <%=homeStartingContent%> </p>


<%posts.forEach(function(post){%>
  <h2>
    <%= post.postTitle %>
  </h2>
  <p>
    <%= (post.postContent).substring(0,100) + "..." %>
    <a href="posts/<%= post._id %>"  >Read More</a>
  </p>
<%})%>



<%- include('partials/footer') %>
